{
  "expo": {
    "name": "vibecode",
    "slug": "vibecode",
    "scheme": "vibecode",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.vibecodeapp.app"
    },
    "android": {
      "package": "com.vibecodeapp.app"
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": ["expo-build-properties", "expo-video"]
  }
}
